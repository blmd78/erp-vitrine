#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/better-sort-package-json@1.1.1/node_modules/better-sort-package-json/bin/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/better-sort-package-json@1.1.1/node_modules/better-sort-package-json/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/better-sort-package-json@1.1.1/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/better-sort-package-json@1.1.1/node_modules/better-sort-package-json/bin/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/better-sort-package-json@1.1.1/node_modules/better-sort-package-json/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/better-sort-package-json@1.1.1/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../better-sort-package-json/bin/cli.js" "$@"
else
  exec node  "$basedir/../better-sort-package-json/bin/cli.js" "$@"
fi
