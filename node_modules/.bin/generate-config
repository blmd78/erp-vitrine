#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules/@nextnode/config-manager/dist/cli/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules/@nextnode/config-manager/dist/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules/@nextnode/config-manager/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules/@nextnode/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules/@nextnode/config-manager/dist/cli/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules/@nextnode/config-manager/dist/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules/@nextnode/config-manager/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules/@nextnode/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/@nextnode+config-manager@3.1.0_astro@5.13.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@_98d7ca6a3472132fd575061aee6d458b/node_modules:/Users/benoitgasnier/Documents/Site/ERP/erp-vitrine/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nextnode/config-manager/dist/cli/generate-types.js" "$@"
else
  exec node  "$basedir/../@nextnode/config-manager/dist/cli/generate-types.js" "$@"
