{
  "name": "@nextnode/functions-client",
  "version": "2.0.0",
  "description": "Client library for Nextnode Functions",
  "keywords": [
    "api",
    "avif",
    "client",
    "functions",
    "image-optimization",
    "lazy-loading",
    "nextnode",
    "responsive-images",
    "typescript",
    "webp"
  ],
  "license": "ISC",
  "author": "Walid M",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist/**/*"
  ],
  "main": "./dist/index.js",
  "dependencies": {
    "@nextnode/logger": "^0.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@nextnode/standards": "^2.1.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.7",
    "@vitest/coverage-v8": "^3.1.4",
    "better-sort-package-json": "^1.1.1",
    "husky": "^9.0.11",
    "jsdom": "^27.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react": "^19.2.0",
    "sharp": "^0.33.5",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "sharp": "^0.33.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./image": {
      "types": "./dist/lib/image/index.d.ts",
      "import": "./dist/lib/image/index.js"
    }
  },
  "module": "./dist/index.js",
  "prettier": "@nextnode/standards/prettier",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "changeset": "changeset",
    "changeset:publish": "changeset publish",
    "changeset:version": "changeset version",
    "clean": "rm -rf dist",
    "commitlint": "commitlint --edit",
    "format": "prettier --write .",
    "lint": "biome check --write --no-errors-on-unmatched",
    "lint-staged": "lint-staged",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  }
}