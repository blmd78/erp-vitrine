#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@6.3.5_@types+node@24.3.0_jiti@2.5.1_lightningcss@1.30.1_terser@5.39.0_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@6.3.5_@types+node@24.3.0_jiti@2.5.1_lightningcss@1.30.1_terser@5.39.0_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
